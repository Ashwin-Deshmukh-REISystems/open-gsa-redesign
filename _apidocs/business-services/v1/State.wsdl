			<complexType name="getListStateResponseType">
				<complexContent>
					<extension base="FPDS:responseType">
						<sequence>
							<element name="listOfStateCodes" type="FPDS:listOfStateCodesType" nillable="true" minOccurs="0"/>
						</sequence>
					</extension>
				</complexContent>
