	<message name="getListInput">
		<part name="authenticationKey" type="FPDS:userAuthenticationKeyType"/>
		<part name="stateCodeSearchCriteria" type="FPDS:stateCodeSearchCriteriaType"/>
	</message>
	<message name="getListOutput">
		<part name="getListStateResponse" type="FPDS:getListStateResponseType"/>
	</message>
	<portType name="StatePortType">
		<operation name="getList">
			<input message="FPDS:getListInput"/>
			<output message="FPDS:getListOutput"/>
		</operation>
	</portType>
	<binding name="StateBinding" type="FPDS:StatePortType">
		<soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
		<operation name="getList">
			<soap:operation soapAction=""/>
			<input>
				<soap:body use="literal" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" namespace="urn:FPDS.BusinessServices.DataCollection.locations.State"/>
			</input>
			<output>
				<soap:body use="literal" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" namespace="urn:FPDS.BusinessServices.DataCollection.locations.State"/>
			</output>
		</operation>
	</binding>
	<service name="State">
		<port name="StatePort" binding="FPDS:StateBinding">
			<soap:address location="https://www.fpds.gov/FPDS/BusinessServices/DataCollection/locations/1.0/State"/>
		</port>
	</service>
