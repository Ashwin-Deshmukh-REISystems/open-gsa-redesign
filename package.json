{
  "name": "open-gsa",
  "version": "1.0.0",
  "description": "GSA Open Technology",
  "main": "index.js",
  "scripts": {
    "eslint:dist": "eslint assets/js/*.js || exit 0",
    "eslint": "npm run eslint:dist",
    "test": "mocha ./test/unit/*",
    "clean:dist": "rm -rf _site",
    "clean": "npm run clean:dist",
    "copy:dist": "cp -R src/ dist/",
    "copy": "npm run copy:dist",
    "prefix": "postcss --use autoprefixer -b 'last 3 versions' --replace _site/assets/css/main.css",
    "sass:dist": "node-sass --output-style compressed -o assets/css assets/css",
    "sass:_site": "node-sass --output-style compressed -o assets/css _site/assets/css",
    "sass": "npm run sass:dist",
    "imagemin": "imagemin assets/img/* --out-dir=_site/assets/img/",
    "pagespeed": "psi http://federalist.18f.gov.s3-website-us-east-1.amazonaws.com/site/gsa/open-gsa-redesign/ --strategy=mobile",
    "accessibility": "a11y http://federalist.18f.gov.s3-website-us-east-1.amazonaws.com/site/gsa/open-gsa-redesign/",
    "build:jekyll": "jekyll build --config _config.yml",
    "build:js": "npm run eslint",
    "build:css": "npm run prefix",
    "build:images": "npm run imagemin",
    "build": "npm run clean && npm run build:js && npm run build:images && npm run build:jekyll && npm run build:css",
    "watch:jekyll": "chokidar 'index.html' '_includes/*.html' '_layouts/*.html' '_posts/*' -c 'npm run build:jekyll && browser-sync reload'",
    "watch:css": "chokidar 'assets/css/*' -c 'npm run browser-sync reload'",
    "watch:js": "chokidar 'assets/js/*' -c 'npm run browser-sync reload'",
    "serve": "browser-sync start --https --server _site",
    "start": "npm run build -s && parallelshell \"npm run serve -s\" \"npm run watch:jekyll -s\" \"npm run watch:css -s\" \"npm run watch:js -s\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GSA/open-gsa-redesign/"
  },
  "author": "GSA Digital Service",
  "contributors": [
    "Sara Cope <sara.cope@gsa.gov>",
    "Jeff Fredrickson <jeff.fredriskson@gsa.gov>",
    "Joe Castle <joseph.castle@gsa.gov>",
    "Ryan Day <ryan.day@gsa.gov>"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/GSA/open-gsa-redesign/issues"
  },
  "homepage": "https://github.com/GSA/open-gsa-redesign/blob/dev/README.md",
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "browser-sync": "^2.11.1",
    "chokidar": "^1.6.1",
    "chokidar-cli": "^1.2.0",
    "eslint": "^1.10.3",
    "eslint-config-google": "^0.3.0",
    "imagemin": "^5.2.2",
    "imagemin-cli": "^3.0.0",
    "node-sass": "^4.5.0",
    "parallelshell": "^2.0.0",
    "postcss": "^5.2.15",
    "postcss-cli": "^2.6.0",
    "watch": "^0.17.1"
  },
  "dependencies": {
    "a11y": "^0.4.0",
    "critical": "^0.7.0",
    "parallelshell": "^2.0.0",
    "psi": "^2.0.2"
  }
}
